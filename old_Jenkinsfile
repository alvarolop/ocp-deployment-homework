pipeline {
	agent any
	environment {
		mvnCmd = "mvn -s ./nexus_openshift_settings.xml"
	}
	stages {
		stage ('Get the git repo'){
			steps {
				echo "Get the git repo"
        			git 'https://github.com/wkulhanek/openshift-tasks.git'	
			}
		}
		stage ('Do something'){
			steps {
				echo "hi"
			}
		}
	}
}
